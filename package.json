{
  "name": "a11y-feedback-monorepo",
  "version": "0.0.0",
  "private": true,
  "description": "Monorepo for a11y-feedback accessibility libraries",
  "type": "module",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "packageManager": "pnpm@8.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm --filter @theaccessibleteam/a11y-feedback dev",
    "test": "pnpm --filter @theaccessibleteam/a11y-feedback test",
    "test:watch": "pnpm --filter @theaccessibleteam/a11y-feedback test:watch",
    "test:coverage": "pnpm --filter @theaccessibleteam/a11y-feedback test:coverage",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "typecheck": "pnpm -r typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "pnpm -r clean && rm -rf node_modules",
    "prepare": "pnpm -r build"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@vitest/coverage-v8": "^1.6.0",
    "axe-core": "^4.10.2",
    "eslint": "^8.57.0",
    "jsdom": "^24.0.0",
    "prettier": "^3.2.0",
    "typescript": "^5.4.0",
    "vite": "^5.2.0",
    "vite-plugin-dts": "^3.8.0",
    "vitest": "^1.4.0"
  },
  "author": "The Accessible Team <theaccessibleteam@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WOLFIEEEE/npm-extention.git"
  },
  "bugs": {
    "url": "https://github.com/WOLFIEEEE/npm-extention/issues"
  },
  "homepage": "https://wolfieeee.github.io/npm-extention/"
}
